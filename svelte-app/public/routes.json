{
  "routes": [
    {
      "route": "/authenticated/*",
      "serve": "/index.html",
      "allowedRoles": ["authenticated"],
      "statusCode": 200
    },
    {
      "route": "/api",
      "serve": "/api",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
